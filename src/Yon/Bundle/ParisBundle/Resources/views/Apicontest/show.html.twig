{% extends 'base.html.twig' %}

{% block body %}
    <h1>ApiContest</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ apiContest.id }}</td>
            </tr>
            <tr>
                <th>Creation</th>
                <td>{% if apiContest.creation %}{{ apiContest.creation|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ apiContest.name }}</td>
            </tr>
            <tr>
                <th>Trophypictureurl</th>
                <td>{{ apiContest.trophyPictureUrl }}</td>
            </tr>
            <tr>
                <th>Participantscount</th>
                <td>{{ apiContest.participantsCount }}</td>
            </tr>
            <tr>
                <th>Autogenerated</th>
                <td>{% if apiContest.autoGenerated %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Enddate</th>
                <td>{% if apiContest.endDate %}{{ apiContest.endDate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Startdate</th>
                <td>{% if apiContest.startDate %}{{ apiContest.startDate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ apiContest.description }}</td>
            </tr>
            <tr>
                <th>Plannedchallengescount</th>
                <td>{{ apiContest.plannedChallengesCount }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('apicontest_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('apicontest_edit', { 'id': apiContest.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
