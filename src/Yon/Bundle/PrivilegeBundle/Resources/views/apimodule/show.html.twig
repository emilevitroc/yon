{% extends 'base.html.twig' %}

{% block body %}
    <h1>ApiModule</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ apiModule.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ apiModule.title }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('apimodule_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('apimodule_edit', { 'id': apiModule.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
